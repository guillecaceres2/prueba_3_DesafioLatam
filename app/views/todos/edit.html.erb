<h1> EDIT To Do <h1> 

<%= form_with model: @todo, local: true do |f| %>
    <%= f.label :description, "nombre To Do" %>
    <%= f.text_field :description %>

    <%= f.label :completed, "tarea completada" %>
    <%= f.check_box :completed %>

    <%= f.submit "enviar" %>

    <%end%>

   
    <%=link_to 'back', todos_path, class: 'btn btn-primary' %>